-- Library

local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()

-- Window

local win = Flux:Window("JioaHub V7", "Pet Simulator X", Color3.fromRGB(120, 220, 0), Enum.KeyCode.LeftControl)

-- Tab

local tab = win:Tab("Farms", "http://www.roblox.com/asset/?id=6023426915")
local script1 = win:Tab("Mastery", "http://www.roblox.com/asset/?id=6023426915")
local script2 = win:Tab("Miscs", "http://www.roblox.com/asset/?id=6023426915")

Flux:Notification("Welcome to JioaHub // This script made by mech#0945 // Thanks for using JioaHub", "Okay")

-- Script

tab:Button("Auto Farm", " ", function()
Flux:Notification("if it's not working change your 4 pets name to 1,2,3,4 and equip it", "Okay")
loadstring(game:HttpGet("https://raw.githubusercontent.com/Brineeee/autofarm/main/lua-to-obfuscate"))()
end)

tab:Line()

tab:Button("Coin Tracter", " ", function() 
loadstring(game:HttpGet('https://pastebin.com/raw/dPXXyp4A'))()
end)

tab:Line()

tab:Button("Triple Damage", " ", function()
local Sound = Instance.new("Sound",game:GetService("SoundService"))
Sound.SoundId = "rbxassetid://232127604"
Sound:Play()
game:GetService("Players").LocalPlayer.PlayerGui.Main.Boosts:FindFirstChild("Triple Damage")
        workspace.__THINGS.__REMOTES["activate boost"]:FireServer({[1] = "Triple Damage"})
    print("Triple damage boost checked")
    game.StarterGui:SetCore("SendNotification", {
    Title = "JioaHub";
    Text = "Triple Damage Activate";
    Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150";
    Duration = 3;
})
end) 

